<h1><%= @post.title %></h1>
<p>
    <%= @post.content %>
</p>
<p>
Categories: 
<!-- <%= @post.categories.map(&:name).join(" ") %></p> -->
 <% @post.categories.each do |cat| %>
 <p> <%=cat.name %></p>
<% end %>
<ul>
<% @post.comments.each do |comment| %>
<li>
    <%= comment.user.username %> says: <%= comment.content %>
</li>
<% end %>
</ul>

